    "next": "15.5.2",
    "next-themes": "0.4.6",
    "react": "18.3.1",
    "react-component-export-image": "1.0.6",
    "react-cookie-consent": "9.0.0",
    "react-dom": "18.3.1",
    "react-ga4": "2.1.0",
    "react-indiana-drag-scroll": "2.2.1",
    "semantic-ui-react": "2.1.5",
