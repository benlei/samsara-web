{
  "name": "samsara",
  "version": "2.3.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9",
    "npm": "use pnpm instead",
    "yarn": "use pnpm instead"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
